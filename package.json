{
  "name": "@whitetrefoil/flickr-simple-reorder-server",
  "version": "0.2.4",
  "description": "A simple tool to help reorder photos in galleries.",
  "main": "lib/common/main.js",
  "module": "lib/esm/main.js",
  "typings": "lib/types/main.d.ts",
  "bin": {
    "flickr-simple-reorder": "bin/flickr-simple-reorder"
  },
  "scripts": {
    "prepublishOnly": "node scripts/prepublish-checklist.js && npm run build",
    "build": "rimraf lib && tsc && tsc -p tsconfig.common.json",
    "test": "npm run build && mocha --recursive tests",
    "coverage": "npm run build && rimraf .nyc_output && nyc mocha --recursive tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/whitetrefoil/flickr-simple-reorder-server.git"
  },
  "bugs": "https://github.com/whitetrefoil/flickr-simple-reorder-server/issues",
  "homepage": "https://github.com/whitetrefoil/flickr-simple-reorder-server",
  "author": "WhiteTrefoil <whitetrefoil@gmail.com>",
  "license": "Apache-2.0",
  "files": [
    "bin/**",
    "lib/**/*.js",
    "lib/types/**/*.d.ts",
    "README.md"
  ],
  "engines": {
    "node": ">=12.18.0"
  },
  "dependencies": {
    "@types/fancy-log": "^1.3.1",
    "@types/fs-extra": "^9.0.1",
    "@types/koa": "^2.11.4",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-conditional-get": "^2.0.0",
    "@types/koa-etag": "^3.0.0",
    "@types/koa-helmet": "^5.2.0",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-router": "^7.4.1",
    "@types/meow": "^5.0.0",
    "@types/statuses": "^2.0.0",
    "@types/through": "0.0.30",
    "@whitetrefoil/debug-log": "^0.1.2",
    "chalk": "^4.1.0",
    "dot-prop": "^5.3.0",
    "fancy-log": "^1.3.3",
    "fs-extra": "^9.0.1",
    "got": "^11.6.2",
    "is-plain-obj": "^2.1.0",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-conditional-get": "^3.0.0",
    "koa-etag": "^4.0.0",
    "koa-helmet": "^5.2.0",
    "koa-logger": "^3.2.1",
    "koa-mount": "^4.0.0",
    "koa-router": "^9.4.0",
    "lodash": "^4.17.20",
    "meow": "^7.1.1",
    "oauth-1.0a": "^2.2.6",
    "statuses": "^2.0.0",
    "through": "^2.3.8",
    "time-stamp": "^2.2.0",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.10.1",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "@whitetrefoil/eslint-config": "^0.13.1",
    "chai": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2"
  },
  "nyc": {
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "lib/api",
      "lib/declarations"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "./test_results",
    "all": true
  }
}
